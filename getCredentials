import jenkins.model.*import com.cloudbees.plugins.credentials.*import com.cloudbees.plugins.credentials.domains.*import hudson.util.Secret// Replace this with the ID of the credential you're looking fordef targetCredentialId = "credId"// Retrieve all credentials from Jenkinsdef creds = com.cloudbees.plugins.credentials.CredentialsProvider.lookupCredentials(    com.cloudbees.plugins.credentials.Credentials.class,    Jenkins.instance,    null,    null)// Find the specific credential by IDdef targetCred = creds.find { it.id == targetCredentialId }if (targetCred) {    // Check the type of the credential and print its details    if (targetCred instanceof com.cloudbees.plugins.credentials.impl.UsernamePasswordCredentialsImpl) {        // Standard Username/Password Credentials        println "ID: ${targetCred.id}, Username: ${targetCred.username}, Password: ${targetCred.password}"    }    if (targetCred instanceof org.jenkinsci.plugins.plaincredentials.impl.StringCredentialsImpl) {        // Secret Text        //println "ID: ${targetCred.id}, Username: ${targetCred.username}, Password: ${targetCred.password}"     println "ID: ${targetCred.id}, Secret: ${targetCred.secret.getPlainText()}"    } } else {    println "Credential with ID '${targetCredentialId}' not found."} 
